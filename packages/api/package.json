{
  "name": "@sorry-cypress/api",
  "version": "0.6.1",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/typings",
  "scripts": {
    "dev": "run-p dev:watch:js generate:watch dev:watch:nodemon",
    "dev:watch:js": "npm run build:js -- --watch",
    "dev:watch:nodemon": "nodemon",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "build": "npm run generate && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline --copy-files",
    "start": "node ./dist/",
    "clean": "rimraf ./dist",
    "test": "echo \"@sorry-cypress/api\"",
    "lint": "eslint ./ --ext ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/cli": "^1.13.4",
    "@graphql-codegen/typescript": "^1.13.4",
    "@graphql-codegen/typescript-resolvers": "^1.13.4",
    "@types/node": "^12.7.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "graphql-tools": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "apollo-server": "2.16.0",
    "dotenv": "^8.1.0",
    "es-abstract": "^1.17.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "husky": "^4.2.5",
    "mongodb": "^3.3.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
